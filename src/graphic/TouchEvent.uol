package graphic;
import sys;
/*
** TouchEvent
*/
public class TouchEvent
{
	define int TOUCH_PRESSED = 1;
	define int TOUCH_RELEASED = 2;
	define int TOUCH_HELD = 3;
	
	private int x;
	private int y;
	
	public int getX()
	{
		return x;
	}
	
	public void setX(int i)
	{
		x = i;
	}
	
	public int getY()
	{
		return y;
	}
	
	public void setY(int i)
	{
		y = i;
	}
	
}